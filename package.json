{
  "name": "react-typescript-order-system",
  "version": "1.0.0",
  "description": "A modern order management system built with React, TypeScript, and Material-UI",
  "type": "module",
  "keywords": [
    "typescript",
    "react",
    "material-ui",
    "order-management"
  ],
  "author": "",
  "license": "MIT",
  "main": "src/index.tsx",
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.15.10",
    "@mui/material": "5.15.10",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "23.4.4",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "13.1.2",
    "react-icons": "4.8.0",
    "react-is": "18.2.0",
    "react-router-dom": "6.10.0",
    "react-window": "1.8.11"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@types/node": "^22.15.18",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-i18next": "^7.8.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "8.49.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "rebuild": "rm -rf dist && tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 8080",
    "clean": "rm -rf dist node_modules",
    "reinstall": "npm run clean && npm install",
    "restart": "npm run clean && npm install && npm run dev",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
